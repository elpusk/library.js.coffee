<?xml version="1.0" encoding="UTF-8" ?>

<!-- lpu237 magnetic card reader setting values -->

<!-- the defined values of attribute

tag	:	attribute	:	values
common	:	interface	: "usb_kb" , "usb_hid" or "rs232"
common	:	buzzer		: "on" or "off"
common	:	language	: "usa_english" , "spanish", "danish", "french", "german", "italian", "norwegian", "swedish", "hebrew" or "turkey"
common	:	iso1		: "enable" or "disable"
common	:	iso2		: "enable" or "disable"	
common	:	iso3		: "enable" or "disable"
common	:	condition	: "and" or "or"
common	:	ibutton		: "zeros", "f12", "zeros7" or "addimat"

global	:	prefix		:
global	:	postfix		:

iso1	:	prefix		:
iso1	:	postfix		:

iso2	:	prefix		:
iso2	:	postfix		:

iso3	:	prefix		:
iso3	:	postfix		:

ibutton	:	prefix		:
ibutton	:	postfix		:

 -->


<lpu237>
	<common
		interface	="usb_kb"
		buzzer		="off"
		language	="usa_english"
		iso1		="enable"
		iso2		="enable"
		iso3		="enable"
		condition	="and"
		ibutton		="zeros"
	/>

	<global
		prefix		="[][0x00][][0x01][][0x02][][0x03][][0x04][][0x05][][0x06]"
		postfix		="[][0x07][][0x08][][0x09][][0x0a][][0x0b][][0x0c][][0x0d]"
	/>
	
	<iso1
		prefix		="[][0x0e][][0x0f][][0x10][][0x11][][0x12][][0x13][][0x14]"
		postfix		="[][0x15][][0x16][][0x17][][0x18][][0x19][][0x1a][][0x1b]"
	/>

	<iso2
		prefix		="[][0x1c][][0x1d][][0x1e][][0x1f][][0x20][][0x21][][0x22]"
		postfix		="[][0x23][][0x24][][0x25][][0x26][][0x27][][0x28][][0x29]"
	/>

	<iso3
		prefix		="[][0x2a][][0x2b][][0x2c][][0x2d][][0x2e][][0x2f][][0x30]"
		postfix		="[][0x31][][0x32][][0x33][][0x34][][0x35][][0x36][][0x37]"
	/>

	<ibutton
		prefix		="[][0x38][][0x39][][0x3a][][0x3b][][0x3c][][0x3d][][0x3e]"
		postfix		="[][0x3f][][0x40][][0x41][][0x42][][0x43][][0x44][][0x45]"
	/>
	
</lpu237>	