iso3	:	add_value1	:  For converting to ASCII code, the value of combination index 1 for iso3 track.
iso3	:	add_value2	:  For converting to ASCII code, the value of combination index 2 for iso3 track.

// attribute "prefix" & "postfix" are old style. Don't use in new project.
iso3	:	prefix		: [i],[o],[p],[[],[]],[\],[del],[z],[x],[c],[v],[b],[n],[m],[,],[.],[/],[`],[1],[2],[3],[4],[5],[6],[7],[8],[9],[0]
iso3	:	postfix		: [-],[=],[bs],[a],[s],[d],[f],[g],[h],[j],[k],[l],[;],['],[enter]

iso3	:	prefix0		: the combination index 0 for iso3 track. equal to "prefix" attribute. "prefix0" will override "prefix" attribute.
iso3	:	prefix1		: the combination index 1 for iso3 track. format is equal to "prefix" attribute.
iso3	:	prefix2		: the combination index 2 for iso3 track. format is equal to "prefix" attribute.

iso3	:	postfix0	: the combination index 0 for iso3 track. equal to "postfix" attribute. "postfix0" will override "postfix" attribute.
iso3	:	postfix1	: the combination index 1 for iso3 track. format is equal to "postfix" attribute.
iso3	:	postfix2	: the combination index 2 for iso3 track. format is equal to "postfix" attribute.

=====================================================================================================================================================
ibutton	:	prefix		: the keycode of USB HID keybaord must be formed to hex.( 0x..)  ex) [][0x2c] - press & release space key.
ibutton	:	postfix		: hex-format : use lower case.
=====================================================================================================================================================
 -->


<lpu237>
	<common
		interface	="usb_kb"
		buzzer		="on"
		language	="usa_english"
		iso1		="enable"
		iso2		="enable"
		iso3		="enable"
		condition	="and"
		indication  ="and"
		ignore1 	= "disable"
		ignore3 	= "disable"
		rm_colon	= "disable"
		direction	="bidirectional"
		ibutton		="zeros"
		mmd1100_reset_interval = "32"
	/>

	<global
		prefix		=""
		postfix		="[][][][]"
	/>

	<iso1
		combination	="1"

		max_size0	="76"
		max_size1	="69"
		max_size2	="76"

		bit_size0	="7"
		bit_size1	="8"
		bit_size2	="7"

		data_mask0	="0xfe"
		data_mask1	="0xff"
		data_mask2	="0xfe"

		use_parity0	="enable"
		use_parity1	="enable"
		use_parity2	="enable"

		parity_type0	="odd"
		parity_type1	="even"
		parity_type2	="odd"

		stxl0		="0x8a"
		stxl1		="0xff"
		stxl2		="0x8a"

		etxl0		="0x3e"
		etxl1		="0xff"
		etxl2		="0x3e"

		use_error_correct0	="enable"
		use_error_correct1	="enable"
		use_error_correct2	="enable"

		error_correct_type0	="lrc"
		error_correct_type1	="invlrc"
		error_correct_type2	="lrc"

		add_value0	="32"
		add_value1	="0"
		add_value2	="32"

		prefix0		="[s][5]"
		postfix0	="[s][/][][enter]"

		prefix1		="[s][5]"
		postfix1	="[s][/][][enter]"

		prefix2		="[s][5]"
		postfix2	="[s][/][][enter]"
	/>

	<iso2
		combination	="1"

		max_size0	="38"
		max_size1	="38"
		max_size2	="38"

		bit_size0	="5"
		bit_size1	="5"
		bit_size2	="5"

		data_mask0	="0xf8"
		data_mask1	="0xf8"
		data_mask2	="0xf8"

		use_parity0	="enable"
		use_parity1	="enable"
		use_parity2	="enable"

		parity_type0	="odd"
		parity_type1	="odd"
		parity_type2	="odd"

		stxl0		="0x58"
		stxl1		="0x58"
		stxl2		="0x58"

		etxl0		="0xf8"
		etxl1		="0xf8"
		etxl2		="0xf8"

		use_error_correct0	="enable"
		use_error_correct1	="enable"
		use_error_correct2	="enable"

		error_correct_type0	="lrc"
		error_correct_type1	="lrc"
		error_correct_type2	="lrc"

		add_value0	="48"
		add_value1	="48"
		add_value2	="48"

		prefix0		="[][;]"
		postfix0	="[s][/][][enter]"

		prefix1		="[][;]"
		postfix1	="[s][/][][enter]"

		prefix2		="[][;]"
		postfix2	="[s][/][][enter]"
	/>

	<iso3
		combination	="1"

		max_size0	="104"
		max_size1	="45"
		max_size2	="104"

		bit_size0	="5"
		bit_size1	="5"
		bit_size2	="5"

		data_mask0	="0xf8"
		data_mask1	="0xf8"
		data_mask2	="0xf8"

		use_parity0	="enable"
		use_parity1	="enable"
		use_parity2	="enable"

		parity_type0	="odd"
		parity_type1	="odd"
		parity_type2	="odd"

		stxl0		="0x58"
		stxl1		="0x58"
		stxl2		="0x58"

		etxl0		="0xf8"
		etxl1		="0xe0"
		etxl2		="0xf8"

		use_error_correct0	="enable"
		use_error_correct1	="enable"
		use_error_correct2	="enable"

		error_correct_type0	="lrc"
		error_correct_type1	="lrc"
		error_correct_type2	="lrc"

		add_value0	="48"
		add_value1	="48"
		add_value2	="48"

		prefix0		="[][;]"
		postfix0	="[s][/][][enter]"

		prefix1		="[][;]"
		postfix1	="[s][/][][enter]"

		prefix2		="[][;]"
		postfix2	="[s][/][][enter]"
	/>

	<ibutton
		prefix		=""
		postfix		="[][enter]"
	/>
	
</lpu237>	